{
  "name": "Perun-web-content",
  "description": "Perun web content manipulation",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "web-content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "api": "http://127.0.0.1:36764/c59e37d6-3e5a-4c95-ae3e-becb0dff2863"
}