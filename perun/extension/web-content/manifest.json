{
  "name": "Perun-web-content",
  "description": "Perun web content manipulation",
  "version": "0.0.1",
  "permissions": [
    "storage",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "web-content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "api": "http://127.0.0.1:36830/408124fe-80f6-41bf-9e3e-604113a6aba6"
}